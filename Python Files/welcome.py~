import PySimpleGUI as sg


def main()h:
    layout = [
        [sg.Image('../Logo.png', size=(200, 200), )],
        [sg.Button('Продължи напред', size=(20, 2))]
    ]

    welcoming_window = sg.Window('Добре дошли в CCP', layout, font='Helvetica 12', size=(400, 300), element_justification='center', icon="../Logo.ico")
    welcoming_window.read()


if __name__ == "__main__":
    main()
